{
  "name": "mobilefood-web",
  "version": "0.1.0",
  "description": "Displays student restaurant foods in Turku.",
  "repository": {
    "type": "git",
    "url": "git://github.com/wiltzu/mobilefood-web.git"
  },
  "scripts": {
    "test": "jest"
  },
  "browser": {
    "brickwork": "./node_modules/brickwork/brickwork.js",
    "brickwork-centering": "./node_modules/brickwork/src/plugins/centering.js",
    "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "ractivate"
    ]
  },
  "browserify-shim": {
